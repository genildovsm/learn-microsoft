{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console","Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
        //"Args": {
        //  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] \"{Application}\" \"{Environment}\" {Message}{NewLine}{Exception}"
        //}
      },
      {
        "Name": "Debug",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/data_.log",
          "rollingInterval": "Day",
          //"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] \"{Application}\" \"{Environment}\" {Message}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Information",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Destructure": [],
    "Properties": {
      "Application": "SerilogDemo",
      "Environment": "Development"
    }
  }
}
